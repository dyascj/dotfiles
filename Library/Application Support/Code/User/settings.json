{
    "security.workspace.trust.untrustedFiles": "newWindow",
    "workbench.colorTheme": "Dracula",
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "claudeCode.preferredLocation": "panel",
    "terminal.integrated.env.osx": {
        "PATH": "${env:HOME}/.local/bin:${env:PATH}"
    },
    "terminal.integrated.smoothScrolling": false,
    "window.titleBarStyle": "custom",

    "vscode_vibrancy.opacity": -1,
    "vscode_vibrancy.theme": "Default Dark",
    "vscode_vibrancy.type": "under-window",

    "workbench.colorCustomizations": {
        "editor.background": "#282a3680",
        "editorGutter.background": "#282a3680",
        "editorLineNumber.foreground": "#6272a4",
        "editorLineNumber.activeForeground": "#f8f8f2",
        "sideBar.background": "#21222c70",
        "sideBar.border": "#44475a",
        "sideBarSectionHeader.background": "#21222c70",
        "activityBar.background": "#21222c70",
        "activityBar.foreground": "#f8f8f2",
        "activityBar.inactiveForeground": "#6272a4",
        "activityBar.border": "#44475a",
        "statusBar.background": "#21222c80",
        "statusBar.border": "#44475a",
        "titleBar.activeBackground": "#21222c70",
        "titleBar.inactiveBackground": "#21222c70",
        "titleBar.border": "#44475a",
        "panel.background": "#21222c70",
        "panel.border": "#44475a",
        "tab.activeBackground": "#282a3680",
        "tab.inactiveBackground": "#21222c70",
        "tab.border": "#44475a",
        "tab.activeBorder": "#bd93f9",
        "editorGroupHeader.tabsBackground": "#21222c70",
        "editorGroupHeader.tabsBorder": "#44475a",
        "dropdown.background": "#21222c99",
        "dropdown.border": "#44475a",
        "input.background": "#21222c99",
        "input.border": "#44475a",
        "quickInput.background": "#21222c99",
        "list.activeSelectionBackground": "#44475a80",
        "list.hoverBackground": "#44475a80",
        "list.inactiveSelectionBackground": "#44475a80",
        "scrollbarSlider.background": "#44475a80",
        "scrollbarSlider.hoverBackground": "#44475a",
        "scrollbarSlider.activeBackground": "#6272a4",
        "terminal.background": "#00000000"
    },
    "terminal.integrated.gpuAcceleration": "off",
    "window.systemColorTheme": "dark",
    "window.autoDetectColorScheme": false
}
